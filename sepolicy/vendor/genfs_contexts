# Display
genfscon sysfs /devices/dsi_panel_driver                                             u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/5e00000.qcom,mdss_mdp/idle_encoder_mask         u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/5e00000.qcom,mdss_mdp/idle_state                u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/5e00000.qcom,mdss_mdp/idle_timeout_ms           u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/virtual/sec                                                  u:object_r:vendor_sysfs_graphics:s0

# USB
genfscon sysfs /devices/platform/soc/4e00000.ssusb/usb_data_enabled                  u:object_r:sysfs_usb_data_enabled:s0

# Wakeup
genfscon sysfs /devices/platform/soc/4a88000.i2c/i2c-1/1-0048/wakeup    u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/soc:et6xx/wakeup                   u:object_r:sysfs_wakeup:s0
